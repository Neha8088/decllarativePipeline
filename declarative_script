pipeline{
  agent any
  stages{
    stage('BuildJob'){
      steps{
        echo "Declarative BuildJob"
      }
    }

    stage('TestJob'){
      steps{
        echo "Declarative TestJob"
      }
    }

    stage('DeployJob'){
      steps{
        echo "Declarative DeployJob"
      }  
    }

    stage('ReleaseJob'){
      steps{
        echo "Declarative ReleaseJob"
      }
    }
  }
}
